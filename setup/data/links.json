    "ranger-config": {
        "source": ".config/ranger/rc.conf",
        "target": "~/.config/ranger/rc.conf"
    },
    "dwm-config": {
        "source": ".config/dwm/config.h",
        "target": "~/.local/repos/dwm/config.h",
        "subs": {
            "main-color": "<sub<main-color>>",
            "secondary-color": "<sub<secondary-color>>",
            "setup-user": "<sub<user>>",
            "dwm-bgcolor": "<sub<dwm-bgcolor>>",
            "dwm-fgcolor": "<sub<dwm-fgcolor>>"
        }
    },
    "plstatus-config": {
        "source": ".config/plstatus/config.json",
        "target": "~/.config/plstatus/config.json"
    },
    "enhancer-for-youtube-config": {
        "source": ".config/enhancer-for-youtube/efy.json",
        "target": "~/.config/enhancer-for-youtube/efy.json",
        "subs": {
            "efy--main-color": "<sub<efy--main-color>>",
            "efy--main-background": "<sub<efy--main-background>>",
            "efy--second-background": "<sub<efy--second-background>>",
