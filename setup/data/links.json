{
    "bashrc": {
        "source": ".bashrc",
        "target": "~/.bashrc"
    },
    "bash-profile": {
        "source": ".bash_profile",
        "target": "~/.bash_profile"
    },
    "dunst-config": {
        "source": ".config/dunst/dunstrc",
        "target": "~/.config/dunst/dunstrc",
        "subs": {
            "main-color": "<sub<main-color>>",
            "dunst-low-bg": "<sub<dunst-low-bg>>",
            "dunst-low-fg": "<sub<dunst-low-fg>>",
            "dunst-normal-bg": "<sub<dunst-normal-bg>>",
            "dunst-normal-fg": "<sub<dunst-normal-fg>>",
