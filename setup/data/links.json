{
    "bashrc": {
        "source": ".bashrc",
        "target": "~/.bashrc"
    },
    "bashProfile": {
        "source": ".bash_profile",
        "target": "~/.bash_profile"
    },
    "dunst": {
        "source": ".config/dunst/dunstrc",
        "target": "~/.config/dunst/dunstrc",
        "subs": {
            "mainColor": "mainColor"
        }
    },
    "alacritty": {
        "source": ".config/alacritty/alacritty.yml",
        "target": "~/.config/alacritty/alacritty.yml"
    },
    "btop": {
        "source": ".config/btop/btop.conf",
        "target": "~/.config/btop/btop.conf"
    },
    "rofi": {
        "source": ".config/rofi/config.rasi",
        "target": "~/.config/rofi/config.rasi",
        "subs": {
            "mainColor": "mainColor"
        }
    },
    "ranger": {
        "source": ".config/ranger/rc.conf",
        "target": "~/.config/ranger/rc.conf"
    },
    "dwm": {
        "source": ".config/dwm/config.h",
        "target": "~/.config/dwm/config.h",
        "subs": {
            "mainColor": "mainColor",
            "secondaryColor": "secondaryColor"
        }
    },
    "plstatus": {
        "source": ".config/plstatus/config.json",
        "target": "~/.config/plstatus/config.json"
    },
    "enhancer-for-youtube": {
        "source": ".config/enhancer-for-youtube/efy.json",
        "target": "~/.config/enhancer-for-youtube/efy.json",
        "subs": {
            "efy--main-color": "efy--main-color",
            "efy--main-background": "efy--main-background",
            "efy--second-background": "efy--second-background",
            "efy--hover-background": "efy--hover-background",
            "efy--main-text": "efy--main-text",
            "efy--dimmer-text": "efy--dimmer-text",
            "efy--shadow": "efy--shadow"
        }
    },
    "reflector": {
        "source": "etc/xdg/reflector/reflector.conf",
        "target": "/etc/xdg/reflector/reflector.conf",
        "flags": ["sudo"]
    },
    "hyprland": {
        "source": ".config/hypr/hyprland.conf",
        "target": "~/.config/hypr/hyprland.conf",
        "subs": {
            "mainColorNoHash": "hyprMainColor",
            "secondaryColorNoHash": "hyprSecondaryColor"
        }
    },
    "hyprpaper": {
        "source": ".config/hypr/hyprpaper.conf",
        "target": "~/.config/hypr/hyprpaper.conf",
        "subs": {
            "wallpaperPath": "hyprWallpaperPath"
        }
    },
    "fuzzel": {
        "source": ".config/fuzzel/fuzzel.ini",
        "target": "~/.config/fuzzel/fuzzel.ini",
        "subs": {
            "mainColorNoHash": "fuzzelColor"
        }
    },
    "waybarConfig": {
        "source": ".config/waybar/config",
        "target": "~/.config/waybar/config"
    },
    "waybarStyle": {
        "source": ".config/waybar/style.css",
        "target": "~/.config/waybar/style.css",
        "subs": {
            "mainColor": "waybarColor"
        }
    },
    "foot": {
        "source": ".config/foot/foot.ini",
        "target": "~/.config/foot/foot.ini",
        "subs": {
            "mainColorNoHash": "footMainColor",
            "secondaryColorNoHash": "footSecondaryColor"
        }
    },
    "firefox": {
        "source": ".mozilla/firefox/profile/chrome/userChrome.css",
        "flags": ["variable-target"],
        "subs": {
            "ffx--button-bgcolor": "ffx--button-bgcolor",
            "ffx--button-hover-bgcolor": "ffx--button-hover-bgcolor",
            "ffx--button-active-bgcolor": "ffx--button-active-bgcolor",
            "ffx--button-primary-bgcolor": "ffx--button-primary-bgcolor",
            "ffx--button-primary-hover-bgcolor": "ffx--button-primary-hover-bgcolor",
            "ffx--button-primary-active-bgcolor": "ffx--button-primary-active-bgcolor",
            "ffx--button-primary-color": "ffx--button-primary-color",
            "ffx--input-bgcolor": "ffx--input-bgcolor",
            "ffx--input-border-color": "ffx--input-border-color",
            "ffx--autocomplete-popup-separator-color": "ffx--autocomplete-popup-separator-color",
            "ffx--panel-banner-item-update-supported-bgcolor": "ffx--panel-banner-item-update-supported-bgcolor",
            "ffx--panel-banner-item-info-icon-bgcolor": "ffx--panel-banner-item-info-icon-bgcolor",
            "ffx--tab-icon-overlay-stroke": "ffx--tab-icon-overlay-stroke",
            "ffx--lwt-accent-color": "ffx--lwt-accent-color",
            "ffx--arrowpanel-background": "ffx--arrowpanel-background",
            "ffx--arrowpanel-border-color": "ffx--arrowpanel-border-color",
            "ffx--toolbar-field-background-color": "ffx--toolbar-field-background-color",
            "ffx--toolbar-field-focus-background-color": "ffx--toolbar-field-focus-background-color",
            "ffx--toolbar-bgcolor": "ffx--toolbar-bgcolor",
            "ffx--toolbarbutton-icon-fill": "ffx--toolbarbutton-icon-fill",
            "ffx--lwt-selected-tab-background-color": "ffx--lwt-selected-tab-background-color",
            "ffx--autocomplete-popup-highlight-background": "ffx--autocomplete-popup-highlight-background"
        }
    },
    "tmux": {
        "source": ".tmux.conf",
        "target": "~/.tmux.conf"
    },
    "swaylock": {
        "source": ".config/swaylock/config",
        "target": "~/.config/swaylock/config",
        "subs": {
            "swl--inside-color": "swl--inside-color",
            "swl--inside-clear-color": "swl--inside-clear-color",
            "swl--inside-ver-color": "swl--inside-ver-color",
            "swl--inside-wrong-color": "swl--inside-wrong-color",
            "swl--key-hl-color": "swl--key-hl-color",
            "swl--bs-hl-color": "swl--bs-hl-color",
            "swl--separator-color": "swl--separator-color",
            "swl--line-color": "swl--line-color",
            "swl--text-color": "swl--text-color",
            "swl--text-clear-color": "swl--text-clear-color",
            "swl--text-caps-lock-color": "swl--text-caps-lock-color",
            "swl--text-ver-color": "swl--text-ver-color",
            "swl--text-wrong-color": "swl--text-wrong-color",
            "swl--ring-color": "swl--ring-color",
            "swl--ring-ver-color": "swl--ring-ver-color",
            "swl--ring-clear-color": "swl--ring-clear-color",
            "swl--ring-wrong-color": "swl--ring-wrong-color"
        }
    }
}
